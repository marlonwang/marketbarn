	<groupId>cc.appmaker</groupId>
	<artifactId>common-sdk</artifactId>
	<version>1.0.0-SNAPSHOT</version>
	<scope>system</scope>
	<systemPath>D:/Program Files/eclipse-jee-luna/workspace/marketbarn/extdir/lib/common-sdk-1.0.0-SNAPSHOT.jar</systemPath>
</dependency>